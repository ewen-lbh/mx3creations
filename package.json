{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "My portfolio",
  "author": "Ewen Le Bihan",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"yarn nuxt-dev\" \"yarn db-dev\"",
    "nuxt-dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "nuxt-generate": "nuxt generate",
    "generate": "concurrently --kill-others \"yarn nuxt-generate\" \"yarn db-generate\"",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest",
    "db-generate": "cd compile-database; poetry run /compile.py",
    "db-dev": "cd compile-database; poetry run ./compile.py --watch --rate 10 --verbose 3"
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "nuxt": "^2.0.0",
    "nuxt-i18n": "^6.5.0",
    "thenby": "^1.3.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "concurrently": "^5.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.0",
    "jest": "^24.1.0",
    "lint-staged": "^10.0.0",
    "prettier": "^1.19.1",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "stylelint": "^10.1.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "vue-jest": "^4.0.0-0"
  }
}
